{
  /*
	// Place your snippets for JavaScript here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	"Print to console": {
		"prefix": "log",
		"body": [
			"console.log('$1');",
			"$2"
		],
		"description": "Log output to console"
	}
*/
  "Print to console": {
    "prefix": "clog",
    "body": ["console.log($1);", "$2"],
    "description": "Log output to console"
  },
  /* Objects */
  "Method": {
    "prefix": "m",
    "body": [
      "${1:class}.prototype.${2:function} = function(${3:params}) {",
      "\t$0",
      "};"
    ],
    "description": "Method"
  },
  "this": {
    "prefix": "t",
    "body": ["this.${1:prop}"],
    "description": "this"
  },
  /* EcmaScript 6 */
  "Object.keys": {
    "prefix": "ok",
    "body": "Object.keys(${1:obj})"
  },
  "forEach loop": {
    "prefix": "fe",
    "body": "${1:iterable}.forEach((${2:item}) => {\n\t${0}\n})"
  },
  /* GSAP */
  "TweenMax": {
    "prefix": "tw",
    "body": ["TweenMax"],
    "description": "TweenMax"
  },
  "TimelineMax": {
    "prefix": "tl",
    "body": ["TimelineMax"],
    "description": "TimelineMax"
  },
  "TweenMax from": {
    "prefix": "twf",
    "body": ["TweenMax.from(${1:element}, ${2:duration}, {${3:vars}});"],
    "description": "TweenMax from"
  },
  "TweenMax to": {
    "prefix": "twt",
    "body": ["TweenMax.to(${1:element}, ${2:duration}, {${3:vars}});"],
    "description": "TweenMax to"
  },
  "TweenMax fromTo": {
    "prefix": "twft",
    "body": [
      "TweenMax.fromTo(${1:element}, ${2:duration}, {${3:fromVars}}, {${4:toVars}});"
    ],
    "description": "TweenMax fromTo"
  },
  "GSAP .set": {
    "prefix": "set",
    "body": ["set(${1:element}, {${2:vars}});"],
    "description": "GSAP set"
  },
  "GSAP .to()": {
    "prefix": ".t",
    "body": [".to(${1:element}, ${2:duration}, {${3:vars}});"],
    "description": "GSAP .to()"
  },
  "GSAP .from()": {
    "prefix": ".f",
    "body": [".from(${1:element}, ${2:duration}, {${3:vars}});"],
    "description": "GSAP .from()"
  },
  "GSAP .fromTo()": {
    "prefix": ".ft",
    "body": [
      ".fromTo(${1:element}, ${2:duration}, {${3:fromVars}}, {${4:toVars}});"
    ],
    "description": "GSAP .fromTo()"
  }
}
